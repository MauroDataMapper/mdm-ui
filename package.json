    "ngx-skeleton-loader": "8.1.0",
    "ngx-toastr": "16.2.0",
    "prismjs": "1.27.0",
    "rxjs": "7.4.0",
    "svg-pan-zoom": "3.6.1",
    "zone.js": "0.13.3"
  },
